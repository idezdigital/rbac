{
    "name": "idez/rbac",
    "description": "A description for rbac.",
    "type": "package",
    "license": "MIT",
    "keywords": [
        "laravel"
    ],
    "authors": [
        {
            "name": "iDez Digital",
            "email": "dev@idez.com.br"
        }
    ],
    "require": {
        "illuminate/support": "^9.0|^10.0|^11.0",
        "idez/nova-checkboxes-field": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "iDez\\Rbac\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "iDez\\Rbac\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "iDez\\Rbac\\Providers\\RbacServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^6.0"
    }
}
